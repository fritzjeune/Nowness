                </optgroup>
                <optgroup id="artibonites-dep" label="Artibonites">
                    <option value="gonaives">Gonaives</option>
                    <option value="st-marc">St-Marc</option>
                    <option value="enery">Enery</option>
                    <option value="dessalines">Dessalines</option>
                    <option value="petit-riviere">Petit-Rivière</option>
                    <option value="marmelade">Mardelade</option>
                    <option value="verette">Verette</option>
                    <option value="gros-morne">Gros-Morne</option>
                </optgroup>
                <optgroup id="nord-dep" label="Nord">
                    <option value="cap-haitien">Cap-haitien</option>
                    <option value="limonade">Limonade</option>
                    <option value="acul-du-nord">Acul-du-nord</option>
