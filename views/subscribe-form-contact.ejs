                    <option value="pignon">Pignon</option>
                    <option value="plaisance">Plaisance</option>
                    <option value="limbe">Limbé</option>
                </optgroup>
                <optgroup id="nord-est-dep" label="Nord-Est">
                    <option value="fort-liberte">Fort-Liberté</option>
                    <option value="terrier">Terrier</option>
                    <option value="ouanaminthe">Ouanaminthe</option>
                    <option value="capotille">Capotille</option>
                    <option value="mont-organisé">Mont-Organisé</option>
                    <option value="trou-du-nord">Trou-du-Nord</option>
                    <option value="terrier-rouge">Terrier-Rouge</option>
                    <option value="valiere">Valière</option>
                </optgroup>
                <optgroup id="nord-ouest-dep" label="Nord-Est">
                    <option value="port-de-paix-dep">Port-de-Paix</option>
                    <option value="latortue-dep">La tortue</option>
                    <option value="bassin-bleu-dep">Bassin-Bleu</option>
                    <option value="saint-louis">Saint-louis</option>
                    <option value="anse-a-foleur-dep">Anse-a-Foleur</option>
                    <option value="bombardopolis">Bombardopolis</option>
                    <option value="jean-rabel">Jean-Rabel</option>
                </optgroup>
                </select><br><br>
                <label for="zip-code">Zip Code</label><input type="text" name="zipCode" id="zip-code">
                <button type="submit">Next</button>
            
            </form>
    </div>



<%- include("footers") %>